{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "src",
    "app",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/CameraComponent.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
